    <Condition Name="ClientAddressDatabaseTableCellDoubleClickCondition" Type="CellDoubleClickCondition" Assembly="Conditions">
      <Table Name="ClientAddressDatabaseTable" />
    </Condition>

    <Condition Name="ClientAddressDatabaseTableSelectedCondition" Type="NotEqualCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="ClientAddressDatabaseTable">
            <Property Name="SelectedRowsCount" />
          </Object>
        </Item>
        <Item>0</Item>
      </Items>
    </Condition>

    <Condition Name="F5KeyDownCondition" Type="KeyDownCondition" Assembly="Conditions">
      <Key Value="F5" />
    </Condition>

    <!-- Постраничный просмотр -->
    <Condition Name="LoanClientCountEqual0Condition" Type="EqualCondition" Assembly="Conditions">
      <Items>
        <Item>
          <DataConnection SourceDataConnection="ClientAddressPrimaryGetDataConnection">
            <Property Name="Count" />
          </DataConnection>
        </Item>
        <Item>0</Item>
      </Items>
      <DataType Type="IntegerDataType" />
    </Condition>

    <Condition Name="CurrentPageEqual1Condition" Type="EqualCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="CurrentPageTextBox" />
        </Item>
        <Item>1</Item>
      </Items>
      <DataType Type="IntegerDataType" />
    </Condition>

    <Condition Name="CurrentPageLess1Condition" Type="LessCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="CurrentPageTextBox" />
        </Item>
        <Item>1</Item>
      </Items>
      <DataType Type="IntegerDataType" />
    </Condition>

    <Condition Name="CurrentPageEqualMaxCondition" Type="EqualCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="CurrentPageTextBox" />
        </Item>
        <Item>
          <DataConnection SourceDataConnection="ClientAddressPrimaryGetDataConnection">
            <Fields>
              <Field Name="TotalPages" />
            </Fields>
          </DataConnection>
        </Item>
      </Items>
      <DataType Type="IntegerDataType" />
    </Condition>

    <Condition Name="CurrentPageGreaterMaxCondition" Type="GreaterCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="CurrentPageTextBox" />
        </Item>
        <Item>
          <DataConnection SourceDataConnection="ClientAddressPrimaryGetDataConnection">
            <Fields>
              <Field Name="TotalPages" />
            </Fields>
          </DataConnection>
        </Item>
      </Items>
      <DataType Type="IntegerDataType" />
    </Condition>

    <Condition Name="CurrentPageTextBoxUpKeyDownCondition" Type="KeyDownCondition" Assembly="Conditions">
      <Object Name="CurrentPageTextBox" />
      <Key Value="Up" />
    </Condition>

    <Condition Name="CurrentPageTextBoxDownKeyDownCondition" Type="KeyDownCondition" Assembly="Conditions">
      <Object Name="CurrentPageTextBox" />
      <Key Value="Down" />
    </Condition>

    <Condition Name="CurrentPageTextBoxEnterKeyDownCondition" Type="KeyDownCondition" Assembly="Conditions">
      <Object Name="CurrentPageTextBox" />
      <Key Value="Enter" />
    </Condition>
  </Conditions>

  <Commands>
    <Command Name="FormCloseCommand" Type="FormCloseCommand" Assembly="Commands" />

