      <Show Type="None" />
      <Parameters>
        <Parameter Name="Edit">True</Parameter>
        <Parameter Name="ClientId">
          <Object Name="ClientAddressDatabaseTable">
            <Property Name="SelectedRowCellValueByColumnName">
              <Parameters>
                <Parameter Name="ColumnName">ClientId</Parameter>
              </Parameters>
            </Property>
          </Object>
        </Parameter>
        <Parameter Name="AddressId">
          <Object Name="ClientAddressDatabaseTable">
            <Property Name="SelectedRowCellValueByColumnName">
              <Parameters>
                <Parameter Name="ColumnName">AddressId</Parameter>
              </Parameters>
            </Property>
          </Object>
        </Parameter>
      </Parameters>
    </Command>

    <Command Name="ClientEditFormShowCommand" Type="FormShowCommand" Assembly="Commands">
      <Xml Type="Path">CollectClientEdit.xml</Xml>
      <Show Type="None" />
      <Multiple Allow="False" Reload="False" />
      <Parameters>
        <Parameter Name="Edit">True</Parameter>
        <Parameter Name="ClientId">
          <Object Name="ClientAddressDatabaseTable">
            <Property Name="SelectedRowCellValueByColumnName">
              <Parameters>
                <Parameter Name="ColumnName">ClientId</Parameter>
              </Parameters>
            </Property>
          </Object>
        </Parameter>
      </Parameters>
    </Command>

    <Command Name="ClientAddressPrimaryGetDataConnectionRefreshCommand" Type="DataConnectionRefreshCommand" Assembly="Commands">
      <DataConnections>
        <DataConnection Name="ClientAddressPrimaryGetDataConnection" />
      </DataConnections>
    </Command>

    <Command Name="UpdatedTrueValueSetCommand" Type="ValueSetCommand" Assembly="Commands">
      <Parameter Name="Updated">True</Parameter>
    </Command>

    <Command Name="ClientAddressSelectRowCommand" Type="ValueSetCommand" Assembly="Commands">
      <Object Name="ClientAddressDatabaseTable">
        <Property Name="SelectRowByFieldValue">
          <Parameters>
            <Parameter Name="ColumnName">AddressId</Parameter>
            <Parameter Name="Value">
              <Input />
            </Parameter>
          </Parameters>
        </Property>
      </Object>
    </Command>

    <Command Name="SelectedAddressIdValueSetCommand" Type="ValueSetCommand" Assembly="Commands">
      <Parameter Name="SelectedAddressId">
        <Object Name="ClientAddressDatabaseTable">
          <Property Name="SelectedRowCellValueByColumnName">
            <Parameters>
              <Parameter Name="ColumnName">AddressId</Parameter>
            </Parameters>
          </Property>
        </Object>
      </Parameter>
    </Command>

    <Command Name="SelectedValueSetCommand" Type="ValueSetCommand" Assembly="Commands">
      <Parameter Name="Selected">True</Parameter>
    </Command>

    <Command Name="SelectAddressSequentialCommand" Type="SequentialCommand" Assembly="Commands">
      <Commands>
        <Command Name="SelectedAddressIdValueSetCommand" />
        <Command Name="SelectedValueSetCommand" />
        <Command Name="FormCloseCommand" />
      </Commands>
    </Command>

    <Command Name="CurrentPageIncreaseCommand" Type="ValueSetCommand" Assembly="Commands">
      <Object Name="CurrentPageTextBox">
        <Formula>
          <Plus DataType="IntegerDataType">
            <Item>
              <Object Name="CurrentPageTextBox" />
            </Item>
            <Item>1</Item>
          </Plus>
        </Formula>
      </Object>
    </Command>

    <Command Name="CurrentPageDecreaseCommand" Type="ValueSetCommand" Assembly="Commands">
      <Object Name="CurrentPageTextBox">
        <Formula>
          <Minus DataType="IntegerDataType">
            <Item>
              <Object Name="CurrentPageTextBox" />
            </Item>
            <Item>1</Item>
          </Minus>
        </Formula>
      </Object>
    </Command>

    <Command Name="CurrentPageTextBoxValueSetCommand" Type="ValueSetCommand" Assembly="Commands">
      <Object Name="CurrentPageTextBox">
        <Input />
      </Object>
    </Command>
  </Commands>

  <Executions>
    <Execution>
      <ConditionExpression>
        <Condition Name="FormClosingCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="FormCloseCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="FormEscapeKeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="FormCloseCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="FormLoadedCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="ClientAddressSelectRowCommand">
          <Parameter Name="AddressId" />
        </Command>
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Command Name="AddressEditFormShowCommand" Parameter="Updated" />
      </ConditionExpression>
      <Commands>
        <Command Name="ClientAddressPrimaryGetDataConnectionRefreshCommand" />
        <Command Name="ClientAddressSelectRowCommand">
          <Command Name="AddressEditFormShowCommand" Parameter="AddressId" />
        </Command>
        <Command Name="UpdatedTrueValueSetCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Command Name="ClientEditFormShowCommand" Parameter="Updated" />
      </ConditionExpression>
      <Commands>
        <Command Name="ClientAddressPrimaryGetDataConnectionRefreshCommand" />
        <Command Name="UpdatedTrueValueSetCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="ClientAddressDatabaseTableCellDoubleClickCondition" />
      </ConditionExpression>
      <Commands>
        <If>
          <When>
            <Parameter Name="SelectionMode" />
          </When>
          <Then>
            <Command Name="SelectAddressSequentialCommand" />
          </Then>
          <Else>
            <Command Name="ClientEditFormShowCommand" />
          </Else>
        </If>
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="F5KeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="ClientAddressPrimaryGetDataConnectionRefreshCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="CurrentPageLess1Condition" />
      </ConditionExpression>
      <Commands>
        <Command Name="CurrentPageTextBoxValueSetCommand">1</Command>
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="CurrentPageGreaterMaxCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="CurrentPageTextBoxValueSetCommand">
          <DataConnection SourceDataConnection="ClientAddressPrimaryGetDataConnection">
            <Fields>
              <Field Name="TotalPages" />
            </Fields>
          </DataConnection>
        </Command>
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="CurrentPageTextBoxUpKeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <If>
          <When>
            <Object Name="NextPageButton">
              <Property Name="Enabled" />
            </Object>
          </When>
          <Then>
            <Command Name="CurrentPageIncreaseCommand" />
            <Command Name="ClientAddressPrimaryGetDataConnectionRefreshCommand" />
          </Then>
        </If>
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="CurrentPageTextBoxDownKeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <If>
          <When>
            <Object Name="PreviousPageButton">
              <Property Name="Enabled" />
            </Object>
          </When>
          <Then>
            <Command Name="CurrentPageDecreaseCommand" />
            <Command Name="ClientAddressPrimaryGetDataConnectionRefreshCommand" />
          </Then>
        </If>
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="CurrentPageTextBoxEnterKeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="ClientAddressPrimaryGetDataConnectionRefreshCommand" />
      </Commands>
    </Execution>
  </Executions>

