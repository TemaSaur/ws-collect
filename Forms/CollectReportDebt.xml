          <Field Name="Archive" />
        </Fields>
      </SqlQuery>
    </DataConnection>

    <DataConnection Name="CessionPrimaryGetDataConnection" Type="PrimaryGetDataConnection" Assembly="DataConnections">
      <SqlQuery Name="CessionSimpleSelectSqlQuery" Type="Select">
        <Workflow Name="Collect" />
        <Fields>
          <Field Name="CessionId" />
          <Field Name="Title" />
        </Fields>
      </SqlQuery>
    </DataConnection>

    <DataConnection Name="InnerOrgPrimaryGetDataConnection" Type="PrimaryGetDataConnection" Assembly="DataConnections">
