  <Executions>
    <Execution>
      <ConditionExpression>
        <Condition Name="FormClosingCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="FormCloseCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="FormEscapeKeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="FormCloseCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="DateStartEnterKeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="RefreshSequentialCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="DateFinishEnterKeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="RefreshSequentialCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Condition Name="F5KeyDownCondition" />
      </ConditionExpression>
      <Commands>
        <Command Name="RefreshSequentialCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <And>
          <Condition Name="ReportDatabaseTableCellDoubleClickCondition" />
          <Condition Name="ReportSelectedOneCondition" />
        </And>
      </ConditionExpression>
      <Commands>
        <Command Name="CollectLoanFormShowCommand" />
      </Commands>
    </Execution>

    <Execution>
      <ConditionExpression>
        <Command Name="CollectLoanFormShowCommand" Parameter="Updated" />
      </ConditionExpression>
      <Commands>
        <Command Name="ReportPrimaryGetDataConnectionRefreshCommand" />
        <Command Name="ShortsDataConnectionRefreshCommand" />
      </Commands>
    </Execution>

  </Executions>

