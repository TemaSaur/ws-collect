            </Value>
          </Parameter>
        </Parameters>
      </SqlQuery>
    </DataConnection>

    <DataConnection Name="CliamantPrimaryGetDataConnection" Type="PrimaryGetDataConnection" Assembly="DataConnections">
      <SqlQuery Name="ClaimantSimpleSelectSqlQuery" Type="Select">
        <Workflow Name="Collect" />
