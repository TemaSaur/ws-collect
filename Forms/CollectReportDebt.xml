  <Conditions>
    <Condition Name="FormClosingCondition" Type="FormClosingCondition" Assembly="Conditions" />

    <Condition Name="FormEscapeKeyDownCondition" Type="KeyDownCondition" Assembly="Conditions">
      <Key Value="Escape" />
    </Condition>

    <Condition Name="DateStartNotGreaterDateFinishCondition" Type="NotGreaterCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="DateStartDateTimePicker" />
        </Item>
        <Item>
          <Object Name="DateFinishDateTimePicker" />
        </Item>
      </Items>
      <DataType Type="DateTimeDataType" />
    </Condition>

    <Condition Name="DateStartEnterKeyDownCondition" Type="KeyDownCondition" Assembly="Conditions">
      <Object Name="DateStartDateTimePicker" />
      <Key Value="Enter" />
    </Condition>

    <Condition Name="DateFinishEnterKeyDownCondition" Type="KeyDownCondition" Assembly="Conditions">
      <Object Name="DateFinishDateTimePicker" />
      <Key Value="Enter" />
    </Condition>

    <Condition Name="F5KeyDownCondition" Type="KeyDownCondition" Assembly="Conditions">
      <Key Value="F5" />
    </Condition>

    <Condition Name="ReportDatabaseTableCellDoubleClickCondition" Type="CellDoubleClickCondition" Assembly="Conditions">
      <Table Name="ReportDatabaseTable" />
    </Condition>

    <Condition Name="RowSelectedCondition" Type="GreaterCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="ReportDatabaseTable">
            <Property Name="SelectedRowsCount" />
          </Object>
        </Item>
        <Item>0</Item>
      </Items>
      <DataType Type="IntegerDataType" />
    </Condition>

    <Condition Name="ReportSelectedOneCondition" Type="EqualCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="ReportDatabaseTable">
            <Property Name="SelectedRowsCount" />
          </Object>
        </Item>
        <Item>1</Item>
      </Items>
      <DataType Type="IntegerDataType" />
    </Condition>

    <Condition Name="CurrentColumnIsPayEqualCondition" Type="EqualCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="ReportDatabaseTable">
            <Property Name="LastCellDoubleClickedColumnName" />
          </Object>
        </Item>
        <Item>TotalPaidInFilters</Item>
      </Items>
      <DataType Type="StringDataType" />
    </Condition>

    <Condition Name="CellClickCondition" Type="CellClickCondition" Assembly="Conditions">
      <Table Name="ReportDatabaseTable" />
    </Condition>

    <Condition Name="ReportEqualCondition" Type="EqualCondition" Assembly="Conditions">
      <Items>
        <Item>
          <Object Name="ReportDatabaseTable">
            <Property Name="LastCellDoubleClickedColumnName" />
          </Object>
        </Item>
        <Item />
      </Items>
    </Condition>
  </Conditions>

  <Commands>
    <Command Name="FormCloseCommand" Type="FormCloseCommand" Assembly="Commands" />

    <Command Name="DateStartVariableSetCommand" Type="ValueSetCommand" Assembly="Commands">
      <Object Name="DateStartVariable">
        <Object Name="DateStartDateTimePicker" />
      </Object>
    </Command>

    <Command Name="DateFinishVariableValueSetCommand" Type="ValueSetCommand" Assembly="Commands">
      <Object Name="DateFinishVariable">
        <Object Name="DateFinishDateTimePicker" />
      </Object>
    </Command>

    <Command Name="ReportPrimaryGetDataConnectionRefreshCommand" Type="DataConnectionRefreshCommand" Assembly="Commands">
      <DataConnections>ReportPrimaryGetDataConnection </DataConnections>
    </Command>

    <Command Name="ShortsDataConnectionRefreshCommand" Type="DataConnectionRefreshCommand" Assembly="Commands">
      <DataConnections>
        <DataConnection Name="InnerOrgPrimaryGetDataConnection" Packet="True" />
        <DataConnection Name="CliamantPrimaryGetDataConnection" Packet="True" />
        <DataConnection Name="CessionPrimaryGetDataConnection" Packet="True" />
      </DataConnections>
    </Command>

    <Command Name="PeriodErrorMessageBoxCommand" Type="MessageBoxCommand" Assembly="Commands">
      <Caption>Обновление отчета</Caption>
      <Text>Дата начала периода фильтрации не должна быть больше даты окончания.</Text>
      <Icon Type="Warning" />
      <Buttons Type="Ok" />
    </Command>

    <Command Name="RefreshSequentialCommand" Type="SequentialCommand" Assembly="Commands">
      <Commands>
        <If>
          <When>
            <Condition Name="DateStartNotGreaterDateFinishCondition" />
          </When>
          <Then>
            <Command Name="DateStartVariableSetCommand" />
            <Command Name="DateFinishVariableValueSetCommand" />
            <Command Name="ReportPrimaryGetDataConnectionRefreshCommand" />
          </Then>
          <Else>
            <Command Name="PeriodErrorMessageBoxCommand" />
          </Else>
        </If>
      </Commands>
    </Command>

    <Command Name="UpdatedTrueValueSetCommand" Type="ValueSetCommand" Assembly="Commands">
      <Parameter Name="Updated">True</Parameter>
    </Command>

    <Command Name="CollectLoanFormShowCommand" Type="FormShowCommand" Assembly="Commands">
      <Xml Type="Path">CollectLoanEdit.xml</Xml>
      <Show Type="None" />
      <Parameters>
        <Parameter Name="LoanId">
          <Object Name="ReportDatabaseTable">
            <Property Name="SelectedRowCellValueByColumnName">
              <Parameters>
                <Parameter Name="ColumnName">LoanId</Parameter>
              </Parameters>
            </Property>
          </Object>
        </Parameter>
        <Parameter Name="LoanClientId">
          <Object Name="ReportDatabaseTable">
            <Property Name="SelectedRowCellValueByColumnName">
              <Parameters>
                <Parameter Name="ColumnName">LoanClientId</Parameter>
              </Parameters>
            </Property>
          </Object>
        </Parameter>
        <Parameter Name="Edit">True</Parameter>
        <Parameter Name="CurrentTab">
          <Switch>
            <Case>
              <When>
                <Condition Name="CurrentColumnIsPayEqualCondition" />
              </When>
              <Then>payment</Then>
            </Case>
            <Case>current_loan</Case>
          </Switch>
        </Parameter>
      </Parameters>
    </Command>

